{
  "name": "activist-home",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:preview": "npm run build && netlify deploy --dir=dist",
    "push-deploy": "git add -A && (git diff --cached --quiet || git commit -m \"update\") && git push && npm run deploy",
    "pull": "git pull origin main",
    "push-deploy:stash": "git stash -u && git pull --rebase origin main || true && git stash pop || true && git add -A && (git diff --cached --quiet || git commit -m \"update\") && git push && npm run deploy"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "gray-matter": "^4.0.3",
    "marked": "^16.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.1",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^7.1.2"
  }
}
